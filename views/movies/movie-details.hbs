<h1>{{title}}</h1>
<h3>Genre: {{genre}}</h3>
<h3>Plot; {{plot}}</h3>
<h3>Cast:</h3>
<ul>
  {{#each cast}}
  <li>
    <p>Name: {{this.name}}</p>
    <p>Ocuppation: {{this.occupation}}</p>
    <p>Catch Phrase: {{this.catchPhrase}}</p>
  </li>
  {{/each}}

  <a href="/movies/{{_id}}/edit">UPDATE</a>
  <br>
  <br>
  <form action="/movies/{{_id}}/delete" method="post">
    <button>DELETE</button>
  </form>
</ul>